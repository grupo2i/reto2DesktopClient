<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="hBox" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="reto2desktopclient.view.AdminMenuController">
    <children>
        <MenuBar fx:id="menuBar">
            <menus>
                <Menu fx:id="menu" mnemonicParsing="false" text="Window">
                    <items>
                        <MenuItem fx:id="menuItemManageClients"
                                  mnemonicParsing="false" text="Manage Clients"
                                  onAction="#handleManageClients"/>
                        <MenuItem fx:id="menuItemManageClubs"
                                  mnemonicParsing="false" text="Manage Clubs"
                                  onAction="#handleManageClubs"/>
                        <MenuItem fx:id="menuItemManageArtists"
                                  mnemonicParsing="false" text="Manage Artists"
                                  onAction="#handleManageArtists"/>
                        <MenuItem fx:id="menuItemManageEvents"
                                  mnemonicParsing="false" text="Manage Events"
                                  onAction="#handleManageEvents"/>
                        <MenuItem mnemonicParsing="false" text="Log Out"
                                  onAction="#handleLogOut"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</HBox>
